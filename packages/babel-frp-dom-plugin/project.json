{
  "name": "babel-frp-dom-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/babel-frp-dom-plugin/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": [
        "{options.outputPath}"
      ],
      "options": {
        "outputPath": "dist/packages/babel-frp-dom-plugin",
        "main": "packages/babel-frp-dom-plugin/src/index.ts",
        "tsConfig": "packages/babel-frp-dom-plugin/tsconfig.lib.json",
        "format": [
          "cjs"
        ],
        "assets": [
          "packages/babel-frp-dom-plugin/*.md"
        ]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "packages/babel-frp-dom-plugin/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/babel-frp-dom-plugin/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "sandbox": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "packages/babel-frp-dom-plugin/jest.config.ts",
        "updateSnapshot": true,
        "testFile": "packages/babel-frp-dom-plugin/test/sandbox.test.ts"
      },
      "configurations": {
        "ci": {
          "ci": false
        }
      }
    }
  },
  "tags": []
}
