{
  "name": "demo",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "demos/demo/src",
  "tags": [],
  "targets": {
    "build": {
      "executor": "@nrwl/webpack:webpack",
      "outputs": [
        "{options.outputPath}"
      ],
      "defaultConfiguration": "production",
      "options": {
        "outputPath": "dist/demos/demo",
        "compiler": "babel",
        "main": "demos/demo/src/main.ts",
        "tsConfig": "demos/demo/tsconfig.app.json",
        "webpackConfig": "demos/demo/webpack.config.js",
        "assets": [
          "demos/demo/src/favicon.ico"
        ],
        "index": "demos/demo/src/index.html",
        "baseHref": "/",
        "scripts": []
      },
      "configurations": {
        "production": {
          "optimization": true,
          "outputHashing": "all",
          "sourceMap": false,
          "namedChunks": false,
          "extractLicenses": true,
          "vendorChunk": false,
          "fileReplacements": [
            {
              "replace": "demos/demo/src/environments/environment.ts",
              "with": "demos/demo/src/environments/environment.prod.ts"
            }
          ]
        }
      }
    },
    "serve": {
      "command": "webpack serve --config demos/demo/webpack.config.ts"
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": [
        "{options.outputFile}"
      ],
      "options": {
        "lintFilePatterns": [
          "demos/demo/**/*.ts"
        ]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": [
        "{workspaceRoot}/coverage/{projectRoot}"
      ],
      "options": {
        "jestConfig": "demos/demo/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  }
}
